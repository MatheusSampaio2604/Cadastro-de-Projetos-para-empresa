@model IEnumerable<Application.ViewModel.GerenciaViewModel>


@{
    ViewData["Title"] = "Gerências";
}

<h1>Lista Gerências</h1>

<p>
    <a asp-action="Create">Criar Novo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_GG)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_GG)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ativo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Nome)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GerenciaGeral.Nome)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Id_GG)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ItemAtivo)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a>
                </td>
            </tr>
        }
    </tbody>
</table>

